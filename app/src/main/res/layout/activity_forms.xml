                   android:layout_marginLeft="10dp"
                   android:layout_width="wrap_content"
                   android:layout_height="match_parent"
                   android:text="DC"
                   android:id="@+id/tdcsDC"
                   android:checked="false" />
               <CheckBox
                   android:layout_marginStart="10dp"
                   android:layout_marginLeft="10dp"
                   android:layout_width="wrap_content"
                   android:layout_height="match_parent"
                   android:text="FU"
                   android:id="@+id/tdcsFU"
                   android:checked="false" />

