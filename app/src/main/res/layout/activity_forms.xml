               <CheckBox
                   android:layout_marginStart="10dp"
                   android:layout_marginLeft="10dp"
                   android:layout_width="wrap_content"
                   android:layout_height="match_parent"
                   android:text="MP"
                   android:id="@+id/BarthelMP"
                   android:checked="false" />
               <CheckBox
