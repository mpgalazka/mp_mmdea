                   android:checked="false" />
               <CheckBox

                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="MP"
                   android:id="@+id/FMMP"
                   android:checked="false" />
               <CheckBox

                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="DC"
                   android:id="@+id/FMDC"
                   android:checked="false" />
               <CheckBox

                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="FU"
                   android:id="@+id/FMFU"
                   android:checked="false" />
           </LinearLayout>


           <Button
